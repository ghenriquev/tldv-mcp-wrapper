# TLDV API Key (obter em: https://tldv.io/app/settings/personal-settings/api-keys)
TLDV_API_KEY=your_tldv_api_key_here

# Porta do servidor HTTP wrapper
PORT=3010

# Path para o servidor MCP do TLDV (se usar Node.js local)
TLDV_MCP_PATH=/path/to/tldv-mcp-server/dist/index.js

# Modo: "docker" ou "node"
MCP_MODE=docker
